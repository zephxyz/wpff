<Window x:Class="WpfApp2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" FontSize="24">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <Label Grid.Row="1" Margin="300 10 300 10" Content="" Name="a" HorizontalAlignment="Center" VerticalAlignment="Center"></Label>
        <Button Grid.Row="3" Margin="100 10 100 10" Click="Button_Click" FontSize="30">Generovat</Button>
        <Label Grid.Row="4" Margin="300 10 300 10" Content="" Name="b" HorizontalAlignment="Center" VerticalAlignment="Center"></Label>
    </Grid>
</Window>
